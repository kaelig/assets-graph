    "pug-loader": "^2.1.0",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "scripts": {
    "test": "jest",
    "prestart": "webpack",
    "start": "NODE_ENV=production ./bin/moniteur serve",
